<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="570.0" prefWidth="785.0" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.Controller.QuestionnaireController">
   <children>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
         <children>
            <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@image-removebg-preview%20(27).png" />
               </image>
            </ImageView>
            <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="96.0" prefWidth="363.0" text="CRITERIAS" textAlignment="CENTER">
               <font>
                  <Font name="System Bold" size="40.0" />
               </font>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Label>
            <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@perfetClassLogo.png" />
               </image>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </ImageView>
         </children>
      </HBox>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="825.0">
               <children>
                  <Label alignment="TOP_CENTER" prefHeight="35.0" prefWidth="342.0" text="Gender equality" textAlignment="CENTER">
                     <font>
                        <Font name="Stencil" size="20.0" />
                     </font>
                  </Label>
                  <ChoiceBox id="genderID" fx:id="equality" prefHeight="23.0" prefWidth="184.0" />
               </children>
            </VBox>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="825.0">
               <children>
                  <Label alignment="TOP_CENTER" prefHeight="35.0" prefWidth="342.0" text="Same City" textAlignment="CENTER">
                     <font>
                        <Font name="Stencil" size="20.0" />
                     </font>
                  </Label>
                  <ChoiceBox id="sameCityID" fx:id="sameCity" prefHeight="23.0" prefWidth="184.0" />
               </children>
            </VBox>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="825.0">
               <children>
                  <Label alignment="TOP_CENTER" prefHeight="35.0" prefWidth="342.0" text="Friends" textAlignment="CENTER">
                     <font>
                        <Font name="Stencil" size="19.0" />
                     </font>
                  </Label>
                  <ChoiceBox id="rateFriendsID" fx:id="Friends" prefHeight="23.0" prefWidth="184.0" />
               </children>
            </VBox>
         </children>
      </HBox>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="825.0">
               <children>
                  <Label alignment="TOP_CENTER" prefHeight="35.0" prefWidth="342.0" text="grades" textAlignment="CENTER">
                     <font>
                        <Font name="Stencil" size="19.0" />
                     </font>
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                  </Label>
                  <ChoiceBox id="grades" fx:id="grades" prefHeight="23.0" prefWidth="184.0">
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets></ChoiceBox>
               </children>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </VBox>
            <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@image-removebg-preview%20(25).png" />
               </image>
            </ImageView>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="825.0">
               <children>
                  <Label alignment="TOP_CENTER" prefHeight="35.0" prefWidth="342.0" text="student type" textAlignment="CENTER">
                     <font>
                        <Font name="Stencil" size="20.0" />
                     </font>
                  </Label>
                  <ChoiceBox id="studentType" fx:id="studentType" prefHeight="23.0" prefWidth="184.0" />
               </children>
            </VBox>
         </children>
      </HBox>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="825.0">
               <children>
                  <Label alignment="TOP_CENTER" prefHeight="35.0" prefWidth="342.0" text="major Preferences" textAlignment="CENTER">
                     <font>
                        <Font name="Stencil" size="20.0" />
                     </font>
                  </Label>
                  <ChoiceBox id="preferencesID" fx:id="preferences" prefHeight="23.0" prefWidth="184.0" />
               </children>
            </VBox>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="825.0">
               <children>
                  <Label alignment="TOP_CENTER" prefHeight="35.0" prefWidth="342.0" text="Special Occasions" textAlignment="CENTER">
                     <font>
                        <Font name="Stencil" size="20.0" />
                     </font>
                  </Label>
                  <ChoiceBox id="specialOccasionsID" fx:id="specialOccasions" prefHeight="23.0" prefWidth="184.0" />
               </children>
            </VBox>
         </children>
      </HBox>
      <Button id="switch to answerID" fx:id="goToPlacement" onAction="#handleGoToPlacementAction" prefHeight="69.0" prefWidth="298.0" text="see placement" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
